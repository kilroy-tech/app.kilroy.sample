{
    "name": "kilroy.sample/new_widget",
    "version": "1",
    "description": "",
    "isInteractive": true,
    "clean_up": true,
    "inputs": [],
    "outputs": [
        "msg",
        "widget_ct"
    ],
    "args": {
        "role": "*"
    },
    "steps": [
        {
            "operation": "set_value",
            "isServer": true,
            "args": {
                "variable": "wfCleanupFlag",
                "value": "1"
            }
        },
        {
            "operation": "step_group",
            "isServer": true,
            "args": {
                "role": "*",
                "message": "",
                "timer": "24",
                "isInteractive": true,
                "canTerminate": true,
                "success": [
                    {
                        "operation": "set_value",
                        "isServer": true,
                        "args": {
                            "variable": "res",
                            "value": "Didn't start!"
                        }
                    },
                    {
                        "operation": "display_inline_form",
                        "isServer": false,
                        "args": {
                            "formID": "default",
                            "form": {
                                "formPanels": [
                                    {
                                        "name": "Start a new widget...",
                                        "description": "",
                                        "sections": [
                                            {
                                                "fields": [
                                                    {
                                                        "name": "description",
                                                        "type": "text",
                                                        "label": "Description",
                                                        "placeholder": ""
                                                    }
                                                ]
                                            }
                                        ]
                                    }
                                ]
                            },
                            "success": [],
                            "failure": []
                        }
                    },
                    {
                        "operation": "app_services",
                        "isServer": true,
                        "args": {
                            "cmd": "start_app_diagram",
                            "pdclass": "widget",
                            "pdalias": "",
                            "pddesc": "${description}",
                            "pdargs": "{}"
                        }
                    },
                    {
                        "operation": "set_var_to",
                        "isServer": true,
                        "args": {
                            "var_name": "res",
                            "push_array_name": null,
                            "field_name": null,
                            "var_expr": {
                                "type": "results",
                                "arg": "__results__"
                            }
                        }
                    },
                    {
                        "operation": "set_value",
                        "isServer": true,
                        "args": {
                            "variable": "msg",
                            "value": "Started ${res}"
                        }
                    }
                ]
            }
        },
        {
            "operation": "end",
            "isServer": true,
            "args": null
        }
    ],
    "externalForms": {}
}